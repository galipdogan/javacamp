package kodlamaIo;

public class Instructor extends User{
	int instructorId;
	String departman;
	String title;//ünvan
	
	public Instructor() {}
	
	public String getDepartman() {
		return departman;
	}
	public void setDepartman(String departman) {
		this.departman = departman;
	}
	public String getAppellation() {
		return appellation;
	}
	public void setAppellation(String appellation) {
		this.appellation = appellation;
	}
	
	public Instructor(int userId, String firtName, String lastName, String eMail, String password,String departman, String title ) {
		this.userId
	}
}
