package kodlamaIo;

public class UserManager {
	public void add(User user) {
		System.out.println("Kullanýcý eklendi" + user.getFisrtName());
	}
	
	public void update(User user) {
		System.out.println("Kullanýcý eklendi" + user.getFisrtName());
	}
	
	public void delete(User user) {
		System.out.println("Kullanýcý eklendi" + user.getFisrtName());
	}
}
